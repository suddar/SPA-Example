<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div id="admin-top-navigator-bar" class="pulldown">
            <a href="#">Home</a>
            <div id="menu-item-users" class="subnav">
                <a class="subnavbtn" href="#">Users<i class="fa fa-caret-down"></i></a>
                <div class="subnav-content">
                    <a href="#">Link</a>
                    <a href="#">Link</a>
                    <a href="#">Link</a>
                </div>
            </div>
            <div class="subnav">
                <a class="subnavbtn" href="/Admin/Resources/">Resources<i class="fa fa-caret-down"></i></a>
                <div class="subnav-content">
                    <a href="/Admin/Resources/Images">Images</a>
                    <a href="/Admin/Resources/Videos">Videos</a>
                    <a href="/Admin/Resources/Audios">Audios</a>
                </div>
            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - WebApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>